MODULE(char);
PROBLEM;
DCL c CHAR(1) INIT('ab');
/*$                ^
error: type mismatch: cannot use constant of type CHAR(2) as initializer for object of type CHAR(1)
*/
DCL d2 CHAR(2) INIT('채'); ! ok; 채 takes two bytes
DCL d CHAR(1) INIT('채'); ! 채 takes 2 bytes!
/*$                ^
error: type mismatch: cannot use constant of type CHAR(2) as initializer for object of type CHAR(1)
*/
MODEND;
