q7: PROC( x REF FIXED);
END;

t7: TASK;
   DCL rf REF FIXED;
   CALL q7(rf);   
   CALL q7(fixed);   
   CALL q7(2);
/*$        ^
error: q7: type mismatch: cannot pass constant of type FIXED(2) as REF FIXED(31)
*/
   CALL q7(f7);
/*$        ^
error: q7: type mismatch: cannot pass object of type FIXED(7) as REF FIXED(31)
*/
   CALL q7(f32);
/*$        ^
error: q7: type mismatch: cannot pass object of type FIXED(32) as REF FIXED(31)
*/
END;

q7i: PROC( x REF INV FIXED);
END;

t7i: TASK;
   DCL if INV FIXED INIT(42);
   DCL rf REF INV FIXED INIT(if);
   CALL q7i(rf);   
   CALL q7i(2); ! pass constant
/*$         ^
error: q7i: type mismatch: cannot pass constant of type FIXED(2) as REF INV FIXED(31)
*/
   CALL q7i(f7);
/*$         ^
error: q7i: type mismatch: cannot pass object of type FIXED(7) as REF INV FIXED(31)
*/
   CALL q7i(1(31));
END;


q8: PROC (x REF CHAR());
