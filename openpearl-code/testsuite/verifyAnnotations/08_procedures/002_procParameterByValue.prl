t8: TASK;
   DCL text1 CHAR(20);
   DCL text2 CHAR(100);
   q8(text1);
   q8(text2);
   q8('Hallo');
/*$   ^
error: q8: type mismatch: cannot pass constant of type CHAR(5) as REF CHAR()
*/
