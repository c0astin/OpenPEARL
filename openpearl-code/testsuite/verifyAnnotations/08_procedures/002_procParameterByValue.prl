MODULE(m1);
/*
check types of procedure parameters with call by value

 happy day scenarios as wells as
 fails with 
   type conflicts
   illegal passing of DATION,SEMA,BOLT and INTERRUPT as Value
   (they may be passed by IDENT or as REF)

 treat all types (with different length).
 INV conflicts may not appear

 Do not treat user defined types and REFs
*/
PROBLEM;

SPC so DATION OUT ALPHIC DIM(*,80) FORWARD GLOBAL(other);
SPC irupt INTERRUPT GLOBAL(other);
DCL b BOLT;
DCL sem SEMA;
   DCL fixed FIXED;
   DCL f7 FIXED(7);
   DCL f32 FIXED(32);
   DCL hFloat FLOAT(52);
   DCL bit2 BIT(2);
   DCL dur DURATION;
   DCL clk CLOCK;
   DCL ch CHAR(2);

q: PROC (f FIXED(31) );
   f := f +1 ;
END;



! call by value with type FIXED(31)
t0: TASK;
   CALL q(2); ! pass constant
   CALL q(f7);
   CALL q(f32);
/*$       ^
error: q: type mismatch: cannot pass object of type FIXED(32) as FIXED(31)
*/

   CALL q(hFloat);
/*$       ^
error: q: type mismatch: cannot pass object of type FLOAT(52) as FIXED(31)
*/

   CALL q(1);
   CALL q(1(32));
/*$       ^
error: q: type mismatch: cannot pass constant of type FIXED(32) as FIXED(31)
*/

   CALL q(bit2);
/*$       ^
error: q: type mismatch: cannot pass object of type BIT(2) as FIXED(31)
*/

   CALL q(dur);
/*$       ^
error: q: type mismatch: cannot pass object of type DURATION as FIXED(31)
*/

   CALL q(clk);
/*$       ^
error: q: type mismatch: cannot pass object of type CLOCK as FIXED(31)
*/

   CALL q(ch);
/*$       ^
error: q: type mismatch: cannot pass object of type CHAR(2) as FIXED(31)
*/

   CALL q(t1);
/*$       ^
error: q: type mismatch: cannot pass object of type TASK as FIXED(31)
*/

   CALL q(so);
/*$       ^
error: q: type mismatch: cannot pass object of type DATION OUT ALPHIC  FORWARD NOCYCL STREAM DIM(*,80) as FIXED(31)
*/

   CALL q(irupt);
/*$       ^
error: q: type mismatch: cannot pass object of type INTERRUPT as FIXED(31)
*/

   CALL q(b);
/*$       ^
error: q: type mismatch: cannot pass object of type BOLT as FIXED(31)
*/

   CALL q(sem);
/*$       ^
error: q: type mismatch: cannot pass object of type SEMA as FIXED(31)
*/

END;

